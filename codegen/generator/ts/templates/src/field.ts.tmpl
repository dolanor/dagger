{{ define "field" }}
	{{- if Solve . }}async {{end}}
	{{- .Name -}}
	({{- template "args" .Args -}})
	{{- if Solve . -}}
		{{ "" }} : Promise<Record<string, string>>{{ "" }}
	{{- else -}}
		{{- with .TypeRef }}
			{{- if .Name }}
				{{- template "return" . -}}
			{{ end -}}
		{{- end -}}
	{{- end -}}
{{ end }}
